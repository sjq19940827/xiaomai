<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!--suppress ALL -->
<mapper namespace="com.discount.dao.ReduceDao">
<update id="updatacoupons" parameterType="com.discount.pojo.TDiscount">
	update t_discount
	<trim prefix="set" suffixOverrides="," suffix="where discountId=#{discountId}">
		<if test="reduceId != null">reduceId=#{reduceId} ,</if>
		<if test="userId != null">userId=#{userId} ,</if>
		<if test="discountMark != null">discountMark=#{discountMark} ,</if>
	</trim>
</update>
</mapper>